<%= form_for [:admin, project, time_log], remote: true do |f| %>
  <div class="col-md-12">
    <div class="form-group">
      <%= f.label :user %>
      <%= f.collection_select :user_id, users_list, :id, :username, {}, { class: "form-control" } %>
    </div>
  </div>

  <div class="col-md-12">
    <div class="form-group">
      <%= f.label :hours, class: "bmd-label-floating" %>
      <%= f.number_field :hours, class: "form-control" %>
      <span id="hoursBlock" class="help-block"></span>
    </div>

  <%= f.submit class: "btn btn-primary pull-right" %>
  </div>
<% end %>
