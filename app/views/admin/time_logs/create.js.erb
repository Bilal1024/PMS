$(".form-group").removeClass("has-danger");
$(".notice").remove();

<% if @time_log.errors.any? %>
  <% if @time_log.errors[:hours].any? %>
    $("#hoursBlock").append('<%= errors_div @time_log, :hours %>');
    $("#hoursBlock").closest(".form-group").addClass("has-danger");
  <% end %>
<% else %>
  <%= render 'shared/notification', flash: flash %>
  $(".time_logs tbody").append('<%=j render "admin/time_logs/time_log", project: @project, time_log: @time_log %>')
  $("#TimeLogModal").modal("toggle");
  $("#TimeLogModal input").each(function (){
    $(this).val("");
  })
<% end %>
